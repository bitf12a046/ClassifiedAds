

@{
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";  
}
  



    <!--Now timeline begin-->
    <div style="margin-left:50px;margin-top:100px" class="row">
        @foreach (var l in ViewBag.data) {
            <div class="col-lg-4">
                <img class="img-thumbnail" src="/account/RetrieveImage/@l.Id" alt=" generic placeholder image" width="400" height="400">
                <h2>@l.Company</h2>
                <p>@l.Description</p>
                <section>
                    <p><br>Prices: @l.Prices<br />Street Address: @l.StreetAddress<br />City: @l.City<br />Phone No: @l.Phone</p>
                </section>
            </div><!-- /.col-lg-4 -->
            
        }

    </div>
